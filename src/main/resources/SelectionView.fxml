         <effect>
            <GaussianBlur radius="2.5" />
         </effect>
      </ImageView>
      <ImageView fx:id="avatar1" fitHeight="484.0" fitWidth="394.0" layoutX="443.0" layoutY="260.0">
         <image>
            <Image url="@Avatars/DSTRUCTOR-FULL.png" />
         </image>
      </ImageView>
      <Button fx:id="select" layoutX="340.0" layoutY="620.0" onAction="#changeScene" prefHeight="70.0" prefWidth="110.0" text="SELECT">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Button fx:id="next" layoutX="830.0" layoutY="620.0" onAction="#nextImage" maxHeight="70.0" prefHeight="70.0" prefWidth="110.0" text=">">
