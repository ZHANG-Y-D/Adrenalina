<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane fx:id="selectionPane" prefHeight="768.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adrenaline.client.view.SelectionViewController">
   <children>
      <Label fx:id="title" alignment="CENTER" layoutX="340.0" prefHeight="150.0" prefWidth="600.0" text="SELECT AVATAR" textAlignment="CENTER" />
      <Button fx:id="close" layoutX="1240.0" layoutY="-2.0" onAction="#close" text="X" />
      <ImageView fx:id="avatar4" fitHeight="185.0" fitWidth="150.0" layoutX="460.0" layoutY="153.0">
         <image>
            <Image url="@Avatars/SPROG_FULL.png" />
         </image>
         <effect>
            <GaussianBlur radius="3.0" />
         </effect>
      </ImageView>
      <ImageView fx:id="avatar3" fitHeight="185.0" fitWidth="150.0" layoutX="670.0" layoutY="153.0">
         <image>
            <Image url="@Avatars/VIOLET_FULL.png" />
         </image>
         <effect>
            <GaussianBlur radius="3.0" />
         </effect>
      </ImageView>
      <ImageView fx:id="avatar2" fitHeight="333.0" fitWidth="271.0" layoutX="729.0" layoutY="218.0">
         <image>
            <Image url="@Avatars/DOZER_FULL.png" />
         </image>
         <effect>
            <GaussianBlur radius="2.5" />
         </effect>
      </ImageView>
      <ImageView fx:id="avatar5" fitHeight="333.0" fitWidth="271.0" layoutX="280.0" layoutY="218.0">
