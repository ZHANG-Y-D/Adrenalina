            <Font size="22.0" />
         </font>
      </Button>
      <StackPane fx:id="stack1" layoutX="205.0" layoutY="115.0" visible="false">
         <children>
            <ImageView fx:id="map1" fitHeight="300.0" fitWidth="395.0" onMouseEntered="#mapHover" onMouseExited="#mapDefault">
               <image>
                  <Image url="@Maps/MAP1.png" />
